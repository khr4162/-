package day05;

import java.util.Scanner;

public class 배열연습1 {

	public static void main(String[] args) {
		/* 5개 값을 가지는 배열을 생성 후
		 * 1 ~ 50 사이의 랜덤 값을 저장 후 출력
		 * 
		 * 
		 * 
		 */
		
		int [] arr = new int[5];
		for(int i=1; i=arr.length; i++) {
			int ran = (int)(Math.random()*50)+1;
			arr[i] = ran;
		System.out.println(arr[i]);
		}
		
		
		
	}

}
