package day05;

public class 배열연습2 {

	public static void main(String[] args) {
		/* 1 ~ 10까지 담고있는 배열을 생성 후 출력
		 * 
		 * 
		 * 
		 */
		int arr[] = new int[10];
		for(int i = 0; i<arr.length; i++) {
			int num = (int)(Math.random()*10)+1;
			arr[i]=num;
			System.out.println(arr[i]);
		}
		for(int j=0; j<arr.length; j++) {
			arr[j]=j+1;
			System.out.print(arr[j]);
		}
		System.out.println();
		
		
		
		
		
		
		
	}

}
