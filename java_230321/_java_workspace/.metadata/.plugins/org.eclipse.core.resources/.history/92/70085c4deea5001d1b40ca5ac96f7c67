package day05;

public class 배열연습4 {

	public static void main(String[] args) {
		/* 2차원 배열
		 * 자료형 배열명[][] = new 자료형[길이][길이];
		 * 
		 * 
		 * 
		 */
		int arr[][] = new int [5][3];
		int cnt = 1;
		//행 갯수가 5개
		for(int i=0; i<arr.length; i++) { //0 ~ 4 5개의 행
			for(int j=0; j<arr[0].length; j++) { //0번째 행에 있는 열 갯수
				arr[i][j] = cnt;
				cnt++;
				System.out.print(arr[i][j]+" ");
			}System.out.println(); //줄바꿈
		}
		//열 갯수가 3개
		
		
		
		
		
		
	}

}
